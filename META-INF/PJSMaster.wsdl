  <wsdl:portType name="PJSMasterPort">
  
        <wsdl:operation name="GetHostsName">
        <wsdl:documentation> 	Gets the hosts name present in the cluster
      				 		 	Possible faults:
					 			-GetHostNameFault if the service cannot be contacted or the are some problems server-side
     	 </wsdl:documentation>
        	<wsdl:input message="tns:GetHostsNameRequest"></wsdl:input>
        	<wsdl:output message="tns:GetHostsNameResponse"></wsdl:output>
        	<wsdl:fault name="fault" message="tns:GetHostsNameFault"></wsdl:fault>
        </wsdl:operation>
        
        <wsdl:operation name="GetHosts">
        <wsdl:documentation> 	Gets the hosts, in the request hosts name have to be specified
      				 			Possible faults:
      				 			-UnknownHost if a non present host is requested
					 			-GetHostsFault if the service cannot be contacted or the are some problems server-side
     	 </wsdl:documentation>
        	<wsdl:input message="tns:GetHostsRequest"></wsdl:input>
        	<wsdl:output message="tns:GetHostsResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:GetHostsFault"></wsdl:fault>
        	<wsdl:fault name="faultUnknownHost" message="tns:UnknownHost"></wsdl:fault>
        	
        </wsdl:operation>
        
        
        <wsdl:operation name="GetJobId">
        <wsdl:documentation> 	Gets the job ids present in the cluster
      					 		Possible faults:
					 			-GetJobIdFault if the service cannot be contacted or the are some problems server-side
