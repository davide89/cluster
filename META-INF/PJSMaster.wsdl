        	<wsdl:output message="tns:JobTerminatedNotificationResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:JobTerminatedNotificationFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
         

  	</wsdl:portType>
  
  

 
