  <wsdl:portType name="PJSJobSubmissionPort">
 
 		<wsdl:operation name="SubmitJob">
 		 <wsdl:documentation>	It is designed to submit a job to the master host
 		 						In the request there are: client (who submits the job), command line, standard in and version (for idempotent method)
 		 						The output is: job id (-1 if it is impossible to dispatch) and a transactionId (to identify uniquely the applicant, if the submission host
 		 						already exist, its id is returned) 
      				 			Possible faults:
      				 			-SubmitJobFault if there are some faults or the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:SubmitJobRequest"></wsdl:input>
        	<wsdl:output message="tns:SubmitJobResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:SubmitJobFault"></wsdl:fault>
        </wsdl:operation>
        
        <wsdl:operation name="SuspendJob">
        <wsdl:documentation>	It is designed to suspend a job
 		 						In the request there are: jobID, client (who is asking for service), version (for idempotent method) and transaction id (who has submitted can suspend, or the admin)
 		 						Possible faults:
      				 			-SuspendFault if there are some faults (i.e. the job does not exist)
      				 			-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:SuspendJobRequest"></wsdl:input>
        	<wsdl:output message="tns:SuspendJobResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:SuspendFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        
        <wsdl:operation name="ResumeJob">
        <wsdl:documentation>	It is designed to resume a job
 		 						In the request there are: jobID, client (who is asking for service), version (for idempotent method) and transaction id (who has submitted can suspend or the admin)
 		 						Possible faults:
      				 			-ResumeFault if there are some faults (i.e. the job does not exist)
      				 			-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:ResumeJobRequest"></wsdl:input>
        	<wsdl:output message="tns:ResumeJobResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:ResumeFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        
        
        
        <wsdl:operation name="KillJob">
        <wsdl:documentation>	It is designed to kill a job
 		 						In the request there are: jobID,  client (who is asking for service), version (for idempotent method) and transaction id (who has submitted or the admin can kill)
 		 						Possible faults:
	      				 		-KillFault if there are some faults (i.e. the job does not exist)
	      				 		-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:KillJobRequest"></wsdl:input>
        	<wsdl:output message="tns:KillJobResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:KillFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        
        
        
        
        
		 <wsdl:operation name="KillJobGroup">
		 
		 <wsdl:documentation>	It is designed to kill a jobgroup
 		 						In the request there are: job group name, client (who is asking for service, only an admin can do that), version (for idempotent method) and transaction id (the admin id)
 		 						Possible faults:
	      				 		-KillGroupFault if there are some faults (i.e. the jobgroup does not exist)
	      				 		-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
     	 
        	<wsdl:input message="tns:KillJobGroupRequest"></wsdl:input>
        	<wsdl:output message="tns:KillJobGroupResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:KillGroupFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        
        
        
        
         <wsdl:operation name="SuspendJobGroup">
        <wsdl:documentation>	It is designed to suspend a jobgroup
 		 						In the request there are: job group name, client (who is asking for service, only an admin can do that), version (for idempotent method) and transaction id (the admin id)
 		 						Possible faults:
	      				 		-SuspendFault if there are some faults (i.e. the jobgroup does not exist)
	      				 		-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:SuspendJobGroupRequest"></wsdl:input>
        	<wsdl:output message="tns:SuspendJobGroupResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:SuspendFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        
        <wsdl:operation name="ResumeJobGroup">
        <wsdl:documentation>	It is designed to resume a jobgroup
 		 						In the request there are: job group name, client (who is asking for service, only an admin can do that), version (for idempotent method) and transaction id (the admin id)
 		 						Possible faults:
	      				 		-ResumeFault if there are some faults (i.e. the jobgroup does not exist)
	      				 		-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:ResumeJobGroupRequest"></wsdl:input>
        	<wsdl:output message="tns:ResumeJobGroupResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:ResumeFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
        

        <wsdl:operation name="ResultJob">
         <wsdl:documentation>	It is designed to get the result of a job
		 		 				In the request there are: job group name, client (who is asking for service, only an admin can do that), version (for idempotent method) and transaction id (who has submitted or the admin can retrieve information)
		 		 				Possible faults:
		      				 	-ResultFault if there are some faults (i.e. the job does not exist or the job is not in DONE status)
		      				 	-ServiceUnavailable if the service cannot be contacted
     	 </wsdl:documentation>
        	<wsdl:input message="tns:ResultJobRequest"></wsdl:input>
        	<wsdl:output message="tns:ResultJobResponse"></wsdl:output>
        	<wsdl:fault name="faultGeneric" message="tns:ResultFault"></wsdl:fault>
        	<wsdl:fault name="ServiceUnavailable" message="tns:ServiceUnavailable"></wsdl:fault>
        </wsdl:operation>
 
  
 
 </wsdl:portType>
 




 

